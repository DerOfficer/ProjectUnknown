animation walk target object
    cond runtimevalue:walking eq 1
        leg1x +3 200
        concurrent leg2x -3 200
        leg1x -6 400
        concurrent leg2x +6 400
        leg1x +3 200
        concurrent leg2x -3 200
    cond end
    call walk
animation end

animation armWalk target object
    cond runtimevalue:walking eq 1
        arm1x -3 200
        concurrent arm2x +3 200
        arm1x +6 400
        concurrent arm2x -6 400
        arm1x -3 200
        concurrent arm2x +3 200
    cond end
    call armWalk
animation end