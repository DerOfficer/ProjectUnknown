animation walk target object
    leg1x +5 50
    concurrent leg2x -5 50
    leg1x -10 50
    concurrent leg2x +10 50
    leg1x +5 50
    concurrent leg2x -5 50
    cond runtimevalue:walking eq 1
        call walk
    cond end
animation end

animation armWalk target object
    arm1x +5 50
    concurrent arm2x -5 50
    arm1x -10 50
    concurrent arm2x +10 50
    arm1x +5 50
    concurrent arm2x -5 50
    cond runtimevalue:walking eq 1
        call armWalk
    cond end
animation end